{
  "roots": [
    "<rootDir>/src"
  ],
  "transform": {
    "^.+\\.ts$": [
      "ts-jest", 
      {
        "tsconfig": "tsconfig.dev.json",
        "diagnostics": false
      }
    ]
  },
  "testRegex": "(test|spec)\\.[jt]sx?$",
  "collectCoverageFrom": [
    "src/**/*.ts"
  ],
  "verbose": false
}
